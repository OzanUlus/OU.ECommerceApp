@model UserDetailViewModel
@{
    Layout = null;
}

<link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />

<div class="container">
    <br />
    <br />
    <br />
    <h3>Kullanıcı Profil Bilgileri</h3>
    <div class="alert alert-info">Kullanıcı Id : @Model.Id</div>
    <div class="alert alert-warning">Kullanıcı Adı : @Model.Name</div>
    <div class="alert alert-success">Kullanıcı Soyadı : @Model.Surname</div>
    <div class="alert alert-primary">Kullanıcı Username :@Model.Username</div>
    <div class="alert alert-secondary">Kullanıcı Mail :@Model.Email</div>
</div>


