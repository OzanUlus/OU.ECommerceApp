@model GetCargoCustomerByCustomerId
@{
    ViewData["Title"] = "UserAddressInfo";
    Layout = "~/Areas/Admin/Views/AdminLayout/Index.cshtml";
}

<section class="content container-fluid">
    <div class="row">


        <div class="col-md-12">
            <div class="chart-box">
               
                
                <div class="bs-example" data-example-id="bordered-table">
            
                    <h3>Kullanıcı Profil Bilgisi</h3>
                    <div class="alert alert-info">Kullanıcı Id:@Model.CargoCustomerId</div>
                    <div class="alert alert-warning">Kullanıcı Adı:@Model.Name</div>
                    <div class="alert alert-info">Telefon:@Model.PhoneNumber</div>
                    <div class="alert alert-warning">Email Adresi: @Model.Email</div>
                    <div class="alert alert-info">İlçe-Şehir: @Model.District-@Model.City</div>
                    <div class="alert alert-warning">Adres: @Model.Address</div>

                </div>
            </div>
        </div>

    </div>
</section>

