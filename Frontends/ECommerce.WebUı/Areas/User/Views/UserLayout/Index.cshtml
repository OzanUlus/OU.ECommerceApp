
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

@await Component.InvokeAsync("_UserLayoutHeadComponentPartial")


<body>
    <div class="container-scroller d-flex">
        
        @await Component.InvokeAsync("_UserLayoutSidebarComponentPartial")

        <div class="container-fluid page-body-wrapper">
            
            @await Component.InvokeAsync("_UserLayoutNavbarComponentPartial")

            @RenderBody()
          
        </div>

    </div>
    <footer class="footer">
        <div class="card">
            <div class="card-body">
                <div class="d-sm-flex justify-content-center justify-content-sm-between">
                    <span class="text-muted d-block text-center text-sm-left d-sm-inline-block">Copyright © O&U ECommerce @DateTime.Now.Year</span>
                    <span class="float-none float-sm-right d-block mt-1 mt-sm-0 text-center"><a>O&U ECommerce Kullanıcı Paneli</a> Tüm Hakları Saklıdır</span>
                </div>
            </div>
        </div>
    </footer>

    <script src="~/spica/spica/vendors/js/vendor.bundle.base.js"></script>
    <script src="~/spica/spica/js/off-canvas.js"></script>
    <script src="~/spica/spica/js/hoverable-collapse.js"></script>
    <script src="~/spica/spica/js/template.js"></script>

</body>

</html>

